#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules/bumpp/bin/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules/bumpp/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules/bumpp/bin/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules/bumpp/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/bumpp@9.2.0/node_modules:/Users/zxdstyle/titan/vite-plugin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/bumpp.js" "$@"
else
  exec node  "$basedir/../../bin/bumpp.js" "$@"
fi
